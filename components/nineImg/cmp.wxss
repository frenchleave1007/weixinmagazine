/* components/nineImg/cmp.wxss */
.container{
  width: 700rpx;
}
.img-container{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  width: 700rpx;
  height: 700rpx;
  position: relative;
}
.image{
  width: 33%;
  height: 31%;
}
.img-num{
  position: absolute;
  right: 80rpx;
  bottom: 80rpx;
  background-color: rgba(0, 0, 0, .7);
  color: #fff;
  font-size: 24rpx;
  padding: 5rpx 10rpx;
}
.text-container{
  padding: 0 30rpx;
  font-size: 40rpx;
  font-weight: 400;
  letter-spacing: 2rpx;
}
